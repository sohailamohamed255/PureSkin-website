<!-- Rose water toner bottle -->
<svg width="400" height="400" viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgRose" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fdf2f8;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#fce7f3;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="400" height="400" fill="url(#bgRose)"/>
  
  <!-- Toner bottle -->
  <rect x="150" y="80" width="100" height="240" rx="15" fill="#ec4899" opacity="0.8"/>
  
  <!-- Spray cap -->
  <rect x="175" y="50" width="50" height="40" rx="25" fill="#be185d" opacity="0.9"/>
  
  <!-- Label -->
  <rect x="160" y="180" width="80" height="80" rx="5" fill="#fdf2f8" opacity="0.9"/>
  <text x="200" y="200" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#be185d">Rose Water</text>
  <text x="200" y="215" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#be185d">Hydrating</text>
  <text x="200" y="230" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#be185d">Toner</text>
  <text x="200" y="250" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" fill="#be185d">Organic Rose</text>
  
  <!-- Rose petals -->
  <ellipse cx="160" cy="120" rx="8" ry="12" fill="#f9a8d4" opacity="0.8" transform="rotate(15 160 120)"/>
  <ellipse cx="240" cy="130" rx="6" ry="10" fill="#f9a8d4" opacity="0.7" transform="rotate(-20 240 130)"/>
  <ellipse cx="180" cy="100" rx="5" ry="8" fill="#f9a8d4" opacity="0.9" transform="rotate(30 180 100)"/>
</svg>