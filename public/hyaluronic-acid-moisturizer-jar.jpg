<!-- Hyaluronic acid moisturizer jar -->
<svg width="400" height="400" viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgHyal" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#eff6ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#dbeafe;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="400" height="400" fill="url(#bgHyal)"/>
  
  <!-- Moisturizer jar -->
  <ellipse cx="200" cy="220" rx="80" ry="50" fill="#3b82f6" opacity="0.8"/>
  
  <!-- Jar lid -->
  <ellipse cx="200" cy="170" rx="80" ry="25" fill="#1d4ed8" opacity="0.9"/>
  
  <!-- Label -->
  <rect x="140" y="200" width="120" height="40" rx="5" fill="#eff6ff" opacity="0.9"/>
  <text x="200" y="215" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#1d4ed8">Hyaluronic Acid</text>
  <text x="200" y="230" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#1d4ed8">Moisturizer</text>
  
  <!-- Water droplets -->
  <circle cx="160" cy="140" r="3" fill="#93c5fd" opacity="0.8"/>
  <circle cx="240" cy="130" r="4" fill="#93c5fd" opacity="0.7"/>
  <circle cx="180" cy="120" r="2" fill="#93c5fd" opacity="0.9"/>
  <circle cx="220" cy="110" r="3" fill="#93c5fd" opacity="0.6"/>
</svg>