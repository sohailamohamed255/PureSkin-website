<!-- Collection of skincare cleansers -->
<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgCleanser" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#eff6ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#dbeafe;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="600" height="400" fill="url(#bgCleanser)"/>
  
  <!-- Cleanser bottles -->
  <rect x="150" y="120" width="60" height="120" rx="30" fill="#3b82f6" opacity="0.8"/>
  <rect x="250" y="140" width="50" height="100" rx="25" fill="#1d4ed8" opacity="0.7"/>
  <rect x="350" y="130" width="55" height="110" rx="27" fill="#2563eb" opacity="0.9"/>
  
  <!-- Pump dispensers -->
  <rect x="170" y="100" width="20" height="30" rx="10" fill="#1e40af" opacity="0.6"/>
  <rect x="270" y="120" width="15" height="25" rx="7" fill="#1e40af" opacity="0.6"/>
  <rect x="370" y="110" width="18" height="28" rx="9" fill="#1e40af" opacity="0.6"/>
  
  <!-- Bubbles/foam effect -->
  <circle cx="200" cy="80" r="8" fill="#bfdbfe" opacity="0.7"/>
  <circle cx="320" cy="90" r="6" fill="#bfdbfe" opacity="0.8"/>
  <circle cx="380" cy="75" r="10" fill="#bfdbfe" opacity="0.6"/>
  <circle cx="180" cy="60" r="5" fill="#bfdbfe" opacity="0.9"/>
  
  <text x="300" y="320" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" fill="#1e40af" font-weight="bold">Cleansers</text>
</svg>