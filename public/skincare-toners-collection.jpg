<!-- Collection of skincare toners -->
<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgToner" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f0fdfa;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ccfbf1;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="600" height="400" fill="url(#bgToner)"/>
  
  <!-- Toner bottles -->
  <rect x="170" y="120" width="50" height="140" rx="8" fill="#14b8a6" opacity="0.8"/>
  <rect x="280" y="110" width="45" height="150" rx="8" fill="#0d9488" opacity="0.9"/>
  <rect x="380" y="125" width="48" height="135" rx="8" fill="#14b8a6" opacity="0.7"/>
  
  <!-- Spray nozzles -->
  <rect x="185" y="100" width="20" height="25" rx="10" fill="#0f766e" opacity="0.8"/>
  <rect x="295" y="90" width="18" height="22" rx="9" fill="#0f766e" opacity="0.8"/>
  <rect x="395" y="105" width="20" height="25" rx="10" fill="#0f766e" opacity="0.8"/>
  
  <!-- Mist effect -->
  <circle cx="195" cy="85" r="2" fill="#5eead4" opacity="0.7"/>
  <circle cx="304" cy="75" r="1.5" fill="#5eead4" opacity="0.8"/>
  <circle cx="405" cy="90" r="2" fill="#5eead4" opacity="0.6"/>
  <circle cx="200" cy="70" r="1" fill="#5eead4" opacity="0.9"/>
  <circle cx="310" cy="60" r="1" fill="#5eead4" opacity="0.7"/>
  <circle cx="410" cy="75" r="1.5" fill="#5eead4" opacity="0.8"/>
  
  <text x="300" y="320" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" fill="#0f766e" font-weight="bold">Toners</text>
</svg>