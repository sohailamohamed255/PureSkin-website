<!-- Natural skincare ingredients and botanicals -->
<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgNatural" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f0fdf4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#dcfce7;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="600" height="400" fill="url(#bgNatural)"/>
  
  <!-- Leaves and botanicals -->
  <ellipse cx="150" cy="100" rx="40" ry="20" fill="#22c55e" opacity="0.7"/>
  <ellipse cx="450" cy="150" rx="35" ry="25" fill="#16a34a" opacity="0.6"/>
  <ellipse cx="300" cy="80" rx="30" ry="15" fill="#15803d" opacity="0.8"/>
  
  <!-- Bottles and containers -->
  <rect x="200" y="200" width="40" height="80" rx="5" fill="#8b5cf6" opacity="0.8"/>
  <rect x="360" y="220" width="35" height="70" rx="5" fill="#06b6d4" opacity="0.7"/>
  <circle cx="480" cy="280" r="25" fill="#f59e0b" opacity="0.6"/>
  
  <!-- Decorative elements -->
  <circle cx="100" cy="300" r="15" fill="#ec4899" opacity="0.5"/>
  <circle cx="500" cy="100" r="20" fill="#8b5cf6" opacity="0.4"/>
  
  <text x="300" y="350" text-anchor="middle" font-family="Arial, sans-serif" font-size="18" fill="#15803d" font-weight="bold">Natural Ingredients</text>
</svg>