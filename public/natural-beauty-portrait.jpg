<!-- Natural beauty portrait -->
<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgPortrait" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f0fdf4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#dcfce7;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="600" height="400" fill="url(#bgPortrait)"/>
  
  <!-- Face silhouette -->
  <ellipse cx="300" cy="200" rx="80" ry="100" fill="#fbbf24" opacity="0.6"/>
  
  <!-- Hair -->
  <ellipse cx="300" cy="120" rx="90" ry="60" fill="#92400e" opacity="0.7"/>
  
  <!-- Eyes -->
  <ellipse cx="280" cy="180" rx="8" ry="5" fill="#1f2937" opacity="0.8"/>
  <ellipse cx="320" cy="180" rx="8" ry="5" fill="#1f2937" opacity="0.8"/>
  
  <!-- Smile -->
  <path d="M280 220 Q300 235 320 220" stroke="#be185d" stroke-width="2" fill="none" opacity="0.8"/>
  
  <!-- Decorative elements -->
  <circle cx="150" cy="100" r="5" fill="#22c55e" opacity="0.6"/>
  <circle cx="450" cy="120" r="8" fill="#16a34a" opacity="0.5"/>
  <circle cx="100" cy="300" r="6" fill="#84cc16" opacity="0.7"/>
  <circle cx="500" cy="320" r="7" fill="#65a30d" opacity="0.6"/>
  
  <!-- Sparkles -->
  <path d="M200 80 L205 90 L210 80 L205 70 Z" fill="#fbbf24" opacity="0.8"/>
  <path d="M400 90 L405 100 L410 90 L405 80 Z" fill="#fbbf24" opacity="0.7"/>
  
  <text x="300" y="360" text-anchor="middle" font-family="Arial, sans-serif" font-size="18" fill="#15803d" font-weight="bold">Natural Beauty</text>
</svg>