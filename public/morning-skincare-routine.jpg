<!-- Morning skincare routine setup -->
<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgMorning" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fffbeb;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#fef3c7;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="600" height="400" fill="url(#bgMorning)"/>
  
  <!-- Bathroom counter -->
  <rect x="50" y="300" width="500" height="60" rx="10" fill="#f3f4f6" opacity="0.8"/>
  
  <!-- Morning products lineup -->
  <rect x="100" y="270" width="25" height="50" rx="5" fill="#3b82f6" opacity="0.8"/>
  <rect x="150" y="260" width="30" height="60" rx="5" fill="#ec4899" opacity="0.8"/>
  <circle cx="220" cy="290" r="20" fill="#f59e0b" opacity="0.8"/>
  <rect x="270" y="265" width="35" height="55" rx="5" fill="#14b8a6" opacity="0.8"/>
  <ellipse cx="350" cy="285" rx="25" ry="15" fill="#8b5cf6" opacity="0.8"/>
  <rect x="400" y="275" width="28" height="45" rx="5" fill="#f97316" opacity="0.8"/>
  
  <!-- Sun rays (morning light) -->
  <path d="M500 50 L510 70 L490 70 Z" fill="#fbbf24" opacity="0.8"/>
  <path d="M520 80 L530 100 L510 100 Z" fill="#fbbf24" opacity="0.7"/>
  <path d="M480 80 L470 100 L490 100 Z" fill="#fbbf24" opacity="0.7"/>
  
  <!-- Water droplets -->
  <circle cx="120" cy="240" r="3" fill="#93c5fd" opacity="0.7"/>
  <circle cx="380" cy="250" r="4" fill="#93c5fd" opacity="0.6"/>
  <circle cx="200" cy="230" r="2" fill="#93c5fd" opacity="0.8"/>
  
  <text x="300" y="380" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#d97706" font-weight="bold">Morning Routine</text>
</svg>