<!-- Modern spa office interior -->
<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgOffice" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8fafc;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e2e8f0;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="600" height="400" fill="url(#bgOffice)"/>
  
  <!-- Reception desk -->
  <rect x="100" y="250" width="200" height="80" rx="10" fill="#64748b" opacity="0.8"/>
  
  <!-- Chairs -->
  <rect x="350" y="200" width="60" height="100" rx="30" fill="#475569" opacity="0.7"/>
  <rect x="450" y="200" width="60" height="100" rx="30" fill="#475569" opacity="0.7"/>
  
  <!-- Plants -->
  <ellipse cx="80" cy="180" rx="20" ry="40" fill="#22c55e" opacity="0.8"/>
  <ellipse cx="520" cy="160" rx="25" ry="50" fill="#16a34a" opacity="0.7"/>
  
  <!-- Window -->
  <rect x="20" y="50" width="150" height="100" rx="5" fill="#bfdbfe" opacity="0.6"/>
  
  <!-- Lighting -->
  <circle cx="300" cy="80" r="25" fill="#fbbf24" opacity="0.5"/>
  
  <text x="300" y="350" text-anchor="middle" font-family="Arial, sans-serif" font-size="18" fill="#475569" font-weight="bold">PureSkin Office</text>
</svg>